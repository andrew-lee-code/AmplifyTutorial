{
  "version": 1,
  "paths": {
    "/todo": {
      "name": "/todo",
      "lambdaFunction": "todoLambda",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    },
    "/user/{id}": {
      "name": "/user/{id}",
      "lambdaFunction": "userLambda",
      "permissions": {
        "setting": "private",
        "auth": [
          "read"
        ]
      }
    }
  }
}